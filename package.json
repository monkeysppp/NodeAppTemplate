{
  "name": "AppTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test":"npm run lint && npm run style && npm run test:unit:coverage",
    "test:unit":"mocha test test/lib",
    "test:unit:coverage":"istanbul cover node_modules/mocha/bin/_mocha -- test test/lib && istanbul report html",
    "lint":"jshint .",
    "style":"jscs ."
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "bunyan": "latest",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "helmet": "^1.1.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^5.5.4",
    "mysql": "latest",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "password-hash-and-salt": "latest",
    "prompt": "latest",
    "serve-favicon": "~2.3.0",
    "uuid4": "^1.0.0"
  },
  "devDependencies":{
    "chai":"latest",
    "istanbul":"latest",
    "jscs":"latest",
    "jshint":"latest",
    "mocha":"latest",
    "node-mocks-http":"latest",
    "sinon":"latest"
  },
  "jscsConfig":{
    "preset": "airbnb",
    "excludeFiles":["node_modules","coverage"]
  }
}
